{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/backend-services/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/backend-services/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/backend-services",
  "projects": {
    "backend-services": {
      "type": "application",
      "root": "apps/backend-services",
      "entryFile": "main",
      "sourceRoot": "apps/backend-services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/backend-services/tsconfig.app.json"
      }
    },
    "user-profile": {
      "type": "application",
      "root": "apps/user-profile",
      "entryFile": "main",
      "sourceRoot": "apps/user-profile/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-profile/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "result": {
      "type": "application",
      "root": "apps/result",
      "entryFile": "main",
      "sourceRoot": "apps/result/src",
      "compilerOptions": {
        "tsConfigPath": "apps/result/tsconfig.app.json"
      }
    },
    "student": {
      "type": "application",
      "root": "apps/student",
      "entryFile": "main",
      "sourceRoot": "apps/student/src",
      "compilerOptions": {
        "tsConfigPath": "apps/student/tsconfig.app.json"
      }
    },
    "faculty": {
      "type": "application",
      "root": "apps/faculty",
      "entryFile": "main",
      "sourceRoot": "apps/faculty/src",
      "compilerOptions": {
        "tsConfigPath": "apps/faculty/tsconfig.app.json"
      }
    },
    "app-admin": {
      "type": "application",
      "root": "apps/app-admin",
      "entryFile": "main",
      "sourceRoot": "apps/app-admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app-admin/tsconfig.app.json"
      }
    }
  }
}